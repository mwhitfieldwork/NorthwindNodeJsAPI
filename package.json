      "sequelize": "^6.35.2",
      "cors": "^2.8.5",
      "helmet": "^7.1.0",
      "morgan": "^1.10.0",
      "express-validator": "^7.0.1",
      "compression": "^1.7.4",
      "dotenv": "^16.3.1",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
      "nodemon": "^3.0.2",
      "jest": "^29.7.0",
      "supertest": "^6.3.3",
      "sequelize-cli": "^6.6.2"
    },
    "engines": {
      "node": ">=18.0.0"
