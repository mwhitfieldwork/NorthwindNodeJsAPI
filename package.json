{
    "name": "northwind-api",
    "version": "1.0.0",
    "description": "Node.js REST API for Northwind Database with MySQL",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "test": "jest",
      "migrate": "npx sequelize-cli db:migrate",
