# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=Northwind
DB_USERNAME=root
DB_PASSWORD=your_password

# Server Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_BASE_URL=/api/v1
CORS_ORIGINS=http://localhost:*,https://yourdomain.com

# Security
JWT_SECRET=your_jwt_secret_key_here
BCRYPT_ROUNDS=12

# Logging
LOG_LEVEL=info

# Swagger Documentation
SWAGGER_TITLE=Northwind API
SWAGGER_VERSION=1.0.0
SWAGGER_DESCRIPTION=REST API for Northwind Database